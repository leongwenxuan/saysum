<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users</title>
</head>
<body>
  <h1>Users</h1>
  <ul>
    <% users.forEach(user => { %>
      <li>
        <h2><%= user.name %> (ID: <%= user.user_id %>)</h2>
        <p>Languages: <%= user.languages.map(lang => lang.language).join(', ') %></p>
        <p>Overall Spent: $<%= user.monthly_spending.overall_spent %></p>
        <h3>Items:</h3>
        <ul>
          <% for (const category in user.items) { %>
            <% if (Array.isArray(user.items[category])) { %>
              <% user.items[category].forEach(item => { %>
                <li><%= category %>: <%= item.name %> - $<%= item.price %></li>
              <% }) %>
            <% } %>
          <% } %>
        </ul>
      </li>
    <% }) %>
  </ul>
</body>
</html>
